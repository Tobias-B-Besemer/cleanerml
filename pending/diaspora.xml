<cleaner id="diaspora">
    <label>Diaspora*</label>
    <description>Social Network software</description>
    <option id="cache">
        <label>Cache</label>
        <description>Delete the cache in tmp directory</description>
        <action command="delete" search="walk.all" path="~/diaspora/tmp/cache/*" />
    </option>
    <option id="log">
        <label>Logs</label>
        <description>Delete the logs</description>
        <action command="delete" search="walk.all" path="~/diaspora/log/*" />
    </option>
</cleaner>